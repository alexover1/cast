main :: () -> int {
    title := "Testing Application";

    InitWindow(800, 600, title.data);
    SetTargetFPS(60);

    bg := RL_DARKBLUE;
    fg := RL_LIME;

    while !WindowShouldClose() {
        BeginDrawing();
        ClearBackground(bg);
        DrawRectangle(0, 0, 100, 100, fg);
        EndDrawing();
    }
    return 0;
}

#load "modules/raylib.ax";
#load "modules/libc.ax";